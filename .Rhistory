prod.fun <- function(Pi,t,Dr) {
df <- NULL
for (i in 0:t) {
Pi = Pi- (Pi*Dr)
#print(paste(round(Pi,0),i+1))
df <- rbind(df,data.frame(i, round(Pi,digits = 0)))
}
names(df)[1] <- "Time"
names(df)[2] <- "Production"
return(as.data.frame(df))
}
prod.fun(1000,10,0.2)
print(df)
